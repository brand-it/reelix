{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://api.themoviedb.org"
        },
        {
          "url": "https://*.themoviedb.org"
        }
      ],
      "deny": []
    },
    "store:default",
    "store:default",
    "shell:default"
  ],
  "platforms": [
    "macOS"
  ]
}